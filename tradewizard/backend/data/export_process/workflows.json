{
  "preparation_phase": [
    {
      "step": "Market research",
      "description": "Conduct comprehensive market research on target export markets",
      "timeline": "2-3 months",
      "resources_required": "Market research consultants, industry reports, trade data",
      "output": "Market overview report with opportunities and challenges",
      "dependencies": "None",
      "cost_estimate": "R35,000-R75,000",
      "key_considerations": [
        "Market size and growth trends",
        "Consumer preferences and behavior",
        "Competitive landscape",
        "Price points and positioning",
        "Distribution channels"
      ]
    },
    {
      "step": "Product adaptation",
      "description": "Adapt products to meet target market requirements and preferences",
      "timeline": "1-3 months",
      "resources_required": "Product development team, regulatory consultants",
      "output": "Market-specific product specifications",
      "dependencies": "Market research completion",
      "cost_estimate": "R50,000-R120,000",
      "key_considerations": [
        "Ingredient modifications if needed",
        "Packaging design and materials",
        "Shelf-life requirements",
        "Labeling compliance",
        "Product positioning"
      ]
    },
    {
      "step": "Certification acquisition",
      "description": "Obtain required certifications for target markets",
      "timeline": "3-6 months",
      "resources_required": "Quality assurance team, certification consultants",
      "output": "Market-specific certificates",
      "dependencies": "Product adaptation completion",
      "cost_estimate": "R85,000-R350,000 depending on certifications required",
      "key_considerations": [
        "Required vs. optional certifications",
        "Certification body selection",
        "Documentation preparation",
        "Facility preparation for audits",
        "Timeline for certification processes"
      ]
    },
    {
      "step": "Partner identification",
      "description": "Identify and evaluate potential distribution partners",
      "timeline": "2-4 months",
      "resources_required": "Business development team, legal counsel",
      "output": "Shortlist of qualified partners with evaluation",
      "dependencies": "Market research completion",
      "cost_estimate": "R65,000-R120,000 including travel",
      "key_considerations": [
        "Partner market coverage and capabilities",
        "Financial stability and track record",
        "Category experience with similar products",
        "Existing retail relationships",
        "Warehousing and logistics capabilities"
      ]
    },
    {
      "step": "Financial planning",
      "description": "Develop export budget and pricing strategy",
      "timeline": "1-2 months",
      "resources_required": "Finance team, pricing consultants",
      "output": "Export budget and pricing model",
      "dependencies": "Market research, partner identification",
      "cost_estimate": "R25,000-R45,000",
      "key_considerations": [
        "Landed cost calculation",
        "Distribution margins",
        "Competitive pricing analysis",
        "Break-even analysis",
        "Cash flow projections"
      ]
    }
  ],
  "documentation_phase": [
    {
      "step": "Export registration",
      "description": "Register as exporter with relevant authorities",
      "timeline": "4-6 weeks",
      "resources_required": "Administrative staff, regulatory consultants",
      "output": "Export registration numbers and permits",
      "dependencies": "None (can run parallel to preparation phase)",
      "cost_estimate": "R8,000-R15,000",
      "key_considerations": [
        "SARS registration as exporter",
        "Industry-specific registrations",
        "Local municipality permits if required",
        "Documentation requirements",
        "Renewal procedures"
      ]
    },
    {
      "step": "Product registration",
      "description": "Register products with target market authorities if required",
      "timeline": "1-3 months",
      "resources_required": "Regulatory affairs team, in-market consultants",
      "output": "Product registration certificates",
      "dependencies": "Export registration, product adaptation",
      "cost_estimate": "R25,000-R60,000 per product",
      "key_considerations": [
        "Market-specific registration requirements",
        "Laboratory testing requirements",
        "Documentation translation if needed",
        "In-market representation requirements",
        "Registration renewal process"
      ]
    },
    {
      "step": "Develop export documentation templates",
      "description": "Create templates for recurring export documentation",
      "timeline": "2-3 weeks",
      "resources_required": "Administrative staff, shipping consultants",
      "output": "Document templates package",
      "dependencies": "Export registration",
      "cost_estimate": "R15,000-R30,000",
      "key_considerations": [
        "Commercial invoice format",
        "Packing list format",
        "Certificate of origin procedures",
        "Customs declaration forms",
        "Market-specific documents"
      ]
    },
    {
      "step": "Establish banking relationships",
      "description": "Set up banking relationships for international trade",
      "timeline": "2-4 weeks",
      "resources_required": "Finance team, banking partners",
      "output": "Trade finance facilities and procedures",
      "dependencies": "None (can run parallel to other steps)",
      "cost_estimate": "Minimal direct costs",
      "key_considerations": [
        "Letter of credit processing capabilities",
        "Currency exchange services",
        "International payment methods",
        "Bank charges structure",
        "Trade finance options"
      ]
    }
  ],
  "logistics_phase": [
    {
      "step": "Shipping provider selection",
      "description": "Select and contract freight forwarders and shipping providers",
      "timeline": "3-4 weeks",
      "resources_required": "Logistics team, procurement",
      "output": "Shipping provider contracts and procedures",
      "dependencies": "Partner identification",
      "cost_estimate": "Minimal direct costs (service fees will be per shipment)",
      "key_considerations": [
        "Experience with perishable goods",
        "Coverage of target markets",
        "Temperature control capabilities",
        "Tracking systems",
        "Documentation handling services"
      ]
    },
